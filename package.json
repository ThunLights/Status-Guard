{
	"name": "status-guard",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"start": "node build/index.js",
		"dev": "vite dev",
		"status-check-bot": "bun run dev/app.ts",
		"build": "npx prisma generate --no-engine && vite build",
		"build:cf": "set \"ADAPTER_TYPE=cf\" && npm run build",
		"ci": "npm run build && npm run build:cf && npm run build:vercel",
		"db:migration": "npx prisma migrate dev",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@fontsource/dela-gothic-one": "^5.2.5",
		"@fontsource/kiwi-maru": "^5.2.5",
		"@fontsource/m-plus-rounded-1c": "^5.2.5",
		"@prisma/adapter-pg": "^6.5.0",
		"@prisma/client": "^6.5.0",
		"@prisma/extension-accelerate": "^1.3.0",
		"@sveltejs/adapter-cloudflare-workers": "^2.9.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@types/bun": "^1.2.8",
		"@types/node": "^22.13.15",
		"bun": "^1.2.8",
		"cloudflare": "^4.2.0",
		"esbuild": "^0.25.2",
		"glob": "^11.0.1",
		"node-fetch": "^3.3.2",
		"wrangler": "^4.5.0",
		"zod": "^3.24.2"
	},
	"engines": {
		"node": "20.x"
	}
}
